{
  "name": "@salto-io/cli",
  "version": "0.1.12",
  "license": "Apache-2.0",
  "description": "cli on top of salto core",
  "repository": {
    "type": "git",
    "url": "https://github.com/salto-io/salto"
  },
  "publishConfig": {
    "access": "public"
  },
  "bin": {
    "salto": "./bin/salto"
  },
  "files": [
    "bin",
    "dist",
    "src"
  ],
  "main": "./bin/salto",
  "types": "dist/index.d.ts",
  "scripts": {
    "prepare": "yarn build",
    "prepublishOnly": "yarn test",
    "generate": "./generate.sh",
    "build": "yarn generate && yarn lint && yarn build-ts",
    "test": "jest",
    "e2e-test": "RUN_E2E_TESTS=1 jest",
    "clean": "rm -rf ./dist ./pkg",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "lint": "eslint --cache --max-warnings 0 --ext .js,.jsx,.ts,.tsx ./",
    "lint-fix": "yarn lint --fix",
    "package": "node ./package_native.js"
  },
  "dependencies": {
    "@salto-io/adapter-api": "0.1.12",
    "@salto-io/adapter-utils": "0.1.12",
    "@salto-io/core": "0.1.12",
    "@salto-io/dag": "0.1.12",
    "@salto-io/logging": "0.1.12",
    "@salto-io/lowerdash": "0.1.12",
    "@salto-io/salesforce-adapter": "0.1.12",
    "chalk": "^2.4.2",
    "figlet": "^1.2.4",
    "inquirer": "^7.0.0",
    "lodash": "^4.17.15",
    "mkdirp": "^0.5.1",
    "moment": "^2.24.0",
    "ora": "^4.0.3",
    "pietile-eventemitter": "^1.0.0",
    "require-directory": "^2.1.1",
    "rimraf": "^3.0.0",
    "source-map-support": "^0.5.13",
    "wu": "^2.1.0",
    "yargonaut": "^1.1.4",
    "yargs": "^13.3.0"
  },
  "devDependencies": {
    "@types/figlet": "^1.2.0",
    "@types/inquirer": "0.0.35",
    "@types/jest": "^24.0.0",
    "@types/mkdirp": "^0.5.2",
    "@types/require-directory": "^2.1.1",
    "@types/rimraf": "^2.0.3",
    "@types/source-map-support": "^0.5.0",
    "@types/tmp": "^0.1.0",
    "@types/yargs": "^13.0.4",
    "eslint": "^6.2.2",
    "eslint-config-airbnb": "18.0.1",
    "eslint-plugin-header": "^3.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^22.6.4",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^1.7.0",
    "jest": "^24.0.0",
    "jest-junit": "^6.4.0",
    "memory-streams": "^0.1.3",
    "nexe": "4.0.0-beta.4",
    "source-map-loader": "^0.2.4",
    "tmp-promise": "^2.0.2",
    "typescript": "3.8.3",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10"
  }
}
