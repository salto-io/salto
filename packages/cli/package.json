{
  "name": "@salto-io/cli",
  "version": "0.3.28",
  "license": "Apache-2.0",
  "description": "cli on top of salto core",
  "repository": {
    "type": "git",
    "url": "https://github.com/salto-io/salto"
  },
  "publishConfig": {
    "access": "public"
  },
  "bin": {
    "salto": "./bin/salto"
  },
  "files": [
    "bin",
    "dist",
    "src"
  ],
  "main": "./bin/salto",
  "types": "dist/index.d.ts",
  "scripts": {
    "clean-ts": "../../build_utils/clean-old-ts.sh",
    "build": "yarn generate && yarn lint && yarn build-ts",
    "test": "jest",
    "build-ts": "tsc -b && yarn clean-ts",
    "watch-ts": "tsc -b -w",
    "lint": "eslint --cache --max-warnings 0 --ext .js,.jsx,.ts,.tsx ./",
    "lint-fix": "yarn lint --fix",
    "clean": "rm -rf ./dist ./pkg .eslintcache src/generated",
    "clean-ts-test": "yarn clean-ts && yarn test",
    "watch-test": "yarn tsc-watch --onSuccess 'yarn clean-ts-test'",
    "e2e-test": "RUN_E2E_TESTS=1 jest",
    "generate": "./generate.sh",
    "package": "node ./package_native.js"
  },
  "dependencies": {
    "@salto-io/adapter-api": "0.3.28",
    "@salto-io/adapter-utils": "0.3.28",
    "@salto-io/core": "0.3.28",
    "@salto-io/dag": "0.3.28",
    "@salto-io/file": "0.3.28",
    "@salto-io/logging": "0.3.28",
    "@salto-io/lowerdash": "0.3.28",
    "@salto-io/salesforce-adapter": "0.3.28",
    "@salto-io/workspace": "0.3.28",
    "chalk": "^2.4.2",
    "commander": "^6.2.1",
    "express": "^4.17.1",
    "figlet": "^1.2.4",
    "glob": "^7.1.6",
    "inquirer": "^7.3.1",
    "lodash": "^4.17.21",
    "moment": "^2.24.0",
    "native-ext-loader": "^2.3.0",
    "nexe-natives": "^1.0.2",
    "null-loader": "^4.0.1",
    "open": "^7.2.0",
    "ora": "^4.0.3",
    "package": "^1.0.1",
    "pietile-eventemitter": "^1.0.0",
    "semver": "^7.3.2",
    "source-map-support": "^0.5.13",
    "wu": "^2.1.0"
  },
  "devDependencies": {
    "@salto-io/e2e-credentials-store": "0.3.28",
    "@salto-io/test-utils": "0.3.28",
    "@types/express": "^4.17.7",
    "@types/inquirer": "7.3.1",
    "@types/jest": "^27.4.0",
    "@types/semver": "^7.3.3",
    "@types/source-map-support": "^0.5.0",
    "@types/supertest": "^2.0.4",
    "@types/tmp": "^0.1.0",
    "@typescript-eslint/eslint-plugin": "4.22.1",
    "@typescript-eslint/parser": "4.22.1",
    "eslint": "^6.2.2",
    "eslint-config-airbnb": "18.0.1",
    "eslint-plugin-header": "^3.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^22.6.4",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^1.7.0",
    "jest": "^27.4.5",
    "jest-circus": "^27.4.5",
    "jest-junit": "^12.0.0",
    "memory-streams": "^0.1.3",
    "nexe": "4.0.0-rc.1",
    "node-loader": "^1.0.2",
    "null-loader": "^4.0.1",
    "raw-loader": "^4.0.2",
    "sinon": "^9.0.3",
    "source-map-loader": "^0.2.4",
    "supertest": "^5.0.0",
    "tmp-promise": "^2.0.2",
    "ts-jest": "^27.1.2",
    "typescript": "4.1.3",
    "wait-for-expect": "^3.0.2",
    "webpack": "^5.11.0",
    "webpack-cli": "^4.3.0"
  }
}
