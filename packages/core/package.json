    "@salto-io/logging": "0.2.15",
    "@salto-io/lowerdash": "0.2.15",
    "@salto-io/marketo-adapter": "0.2.15",
    "@salto-io/netsuite-adapter": "0.2.15",
    "@salto-io/salesforce-adapter": "0.2.15",
    "@salto-io/workato-adapter": "0.2.15",
    "@salto-io/workspace": "0.2.15",
    "@salto-io/zuora-billing-adapter": "0.2.15",
    "axios": "^0.21.1",
    "glob": "^7.1.6",
    "lodash": "^4.17.21",
    "pietile-eventemitter": "^1.0.0",
    "readdirp": "^3.1.1",
    "semver": "^7.3.2",
    "uuid": "^3.3.3",
    "wu": "^2.1.0"
  },
  "devDependencies": {
    "@types/jest": "^26.0.20",
    "@types/lodash": "^4.14.168",
    "@types/node": "^12.7.1",
    "@types/tmp": "^0.1.0",
