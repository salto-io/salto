{
  "name": "@salto-io/core",
  "version": "0.1.1",
  "license": "Apache-2.0",
  "description": "Salto core",
  "repository": {
    "type": "git",
    "url": "https://github.com/salto-io/salto"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "bin/**/*",
    "dist/**/*"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "prepare": "yarn run build",
    "prepublishOnly": "yarn test",
    "build": "yarn run lint  && yarn run build-ts && yarn run build-parser",
    "test": "jest",
    "clean": "rm -rf ./dist",
    "clean-ts": "node ./node_modules/.bin/ts-cleaner -d dist -s . -v",
    "clean-ts-test": "yarn run clean-ts && yarn run test",
    "watch-test": "yarn run tsc-watch --onSuccess 'yarn run clean-ts-test'",
    "build-ts": "tsc",
    "build-parser": "yarn run nearleyc src/parser/internal/hcl.ne -o dist/src/parser/internal/hcl.js",
    "watch-ts": "tsc -w",
    "lint": "eslint --cache --max-warnings 0 --ext .js,.jsx,.ts,.tsx ./",
    "lint-fix": "yarn run lint --fix"
  },
  "dependencies": {
    "@salto-io/adapter-api": "0.1.1",
    "@salto-io/dag": "0.1.1",
    "@salto-io/hubspot-adapter": "0.1.1",
    "@salto-io/logging": "0.1.1",
    "@salto-io/lowerdash": "0.1.1",
    "@salto-io/salesforce-adapter": "0.1.1",
    "async": "^3.1.0",
    "chalk": "^2.3.0",
    "csv-parser": "^2.3.2",
    "fuse.js": "^3.4.5",
    "json2csv": "^4.5.2",
    "lodash": "^4.17.11",
    "mkdirp": "^0.5.1",
    "moo": "^0.5.1",
    "nearley": "2.19.0",
    "pietile-eventemitter": "^1.0.0",
    "readdirp": "^3.1.1",
    "rimraf": "^3.0.0",
    "uuid": "^3.3.3",
    "wu": "^2.1.0",
    "@types/rimraf": "^2.0.3"
  },
  "devDependencies": {
    "@types/async": "^3.0.0",
    "@types/jest": "^24.0.0",
    "@types/json2csv": "^4.5.0",
    "@types/lodash": "^4.14.133",
    "@types/mkdirp": "^0.5.2",
    "@types/moo": "^0.5.0",
    "@types/nearley": "^2.11.0",
    "@types/node": "^12.7.1",
    "@types/supertest": "^2.0.4",
    "@types/tmp": "^0.1.0",
    "@types/uuid": "^3.4.5",
    "@types/wu": "^2.1.40",
    "@typescript-eslint/eslint-plugin": "^2.6.1",
    "@typescript-eslint/parser": "^2.6.1",
    "eslint": "^6.2.2",
    "eslint-config-airbnb": "18.0.1",
    "eslint-plugin-header": "^3.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^22.6.4",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^1.7.0",
    "jest": "^24.0.0",
    "jest-circus": "^24.9.0",
    "jest-junit": "^6.4.0",
    "tmp-promise": "^2.0.2",
    "ts-cleaner": "^1.0.2",
    "tsc-watch": "^2.2.1",
    "typescript": "^3.7.2"
  }
}
