{
  "name": "@salto-io/dummy-adapter",
  "version": "0.3.28",
  "license": "Apache-2.0",
  "description": "Salto Salesforce adapter",
  "repository": {
    "type": "git",
    "url": "https://github.com/salto-io/salto_monorepo"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist",
    "src",
    "e2e_test",
    "index.ts"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "yarn generate && yarn lint && yarn build-ts",
    "test": "jest",
    "clean": "rm -rf ./dist .eslintcache",
    "clean-ts": "../../build_utils/clean-old-ts.sh",
    "clean-ts-test": "yarn clean-ts && yarn test",
    "watch-test": "yarn tsc-watch --onSuccess 'yarn clean-ts-test'",
    "build-ts": "tsc -b",
    "watch-ts": "tsc -b -w",
    "lint": "eslint --cache --max-warnings 0 --ext .js,.jsx,.ts,.tsx ./",
    "lint-fix": "yarn lint --fix",
    "generate": "./generate.sh"
  },
  "dependencies": {
    "@salto-io/adapter-api": "0.3.28",
    "@salto-io/adapter-utils": "0.3.28",
    "@salto-io/lowerdash": "0.3.28",
    "@salto-io/workspace": "0.3.28",
    "lodash": "^4.17.21",
    "readdirp": "^3.1.1",
    "seedrandom": "^3.0.5",
    "unique-names-generator": "^4.3.1"
  },
  "devDependencies": {
    "@types/jest": "^27.4.0",
    "@types/seedrandom": "^2.4.28",
    "@typescript-eslint/eslint-plugin": "4.22.1",
    "@typescript-eslint/parser": "4.22.1",
    "eslint": "^6.2.2",
    "eslint-config-airbnb": "18.0.1",
    "eslint-plugin-header": "^3.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^22.6.4",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^1.7.0",
    "jest": "^27.4.5",
    "jest-circus": "^27.4.5",
    "jest-junit": "^12.0.0",
    "ts-jest": "^27.1.2"
  }
}
