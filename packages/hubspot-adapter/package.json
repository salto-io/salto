{
  "name": "@salto/hubspot-adapter",
  "version": "0.1.0",
  "description": "Salto Hubspot adapter",
  "repository": {
    "type": "git",
    "url": "https://github.com/salto-io/salto_monorepo"
  },
  "private": true,
  "files": [
    "dist/**/*"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "prepare": "yarn run build",
    "prepublishOnly": "yarn test",
    "build": "yarn run lint && yarn run build-ts",
    "test": "jest --forceExit --detectOpenHandles",
    "clean": "rm -rf ./dist",
    "clean-ts": "node ./node_modules/.bin/ts-cleaner -d dist -s . -v",
    "clean-ts-test": "yarn run clean-ts && yarn run test",
    "watch-test": "yarn run tsc-watch --onSuccess 'yarn run clean-ts-test'",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "lint": "eslint --cache --max-warnings 0 --ext .js,.jsx,.ts,.tsx ./",
    "lint-fix": "yarn run lint --fix"
  },
  "dependencies": {
    "@salto/logging": "0.1.0",
    "@salto/lowerdash": "0.1.0",
    "@types/request-promise": "^4.1.45",
    "adapter-api": "0.1.0",
    "buffer": "^5.2.1",
    "events": "^3.0.0",
    "hubspot": "^2.3.4",
    "lodash": "^4.17.11",
    "requestretry": "^4.0.2",
    "stream": "0.0.2",
    "winston": "^3.2.1",
    "wu": "^2.1.0"
  },
  "devDependencies": {
    "@types/jest": "^22.1.3",
    "@types/lodash": "^4.14.133",
    "@types/node": "^12.7.1",
    "@typescript-eslint/eslint-plugin": "^2.6.1",
    "@typescript-eslint/parser": "^2.6.1",
    "eslint": "^6.2.2",
    "eslint-config-airbnb": "18.0.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^22.6.4",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^1.7.0",
    "jest": "^24.0.0",
    "jest-circus": "^24.9.0",
    "jest-junit": "^6.4.0",
    "request": "^2.88.0",
    "ts-cleaner": "^1.0.3",
    "tsc-watch": "^2.2.1",
    "typescript": "^3.7.2"
  }
}
