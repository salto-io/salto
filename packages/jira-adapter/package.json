{
  "name": "@salto-io/jira-adapter",
  "version": "0.3.19",
  "license": "Apache-2.0",
  "description": "Salto JIRA adapter",
  "repository": {
    "type": "git",
    "url": "https://github.com/salto-io/salto"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist",
    "src",
    "index.ts"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "yarn concurrently \"yarn lint\" \"yarn build-ts\"",
    "test": "jest",
    "e2e-test": "RUN_E2E_TESTS=1 jest",
    "clean": "rm -rf ./dist .eslintcache",
    "clean-ts": "../../build_utils/clean-old-ts.sh",
    "clean-ts-test": "yarn clean-ts && yarn test",
    "watch-test": "yarn tsc-watch --onSuccess 'yarn clean-ts-test'",
    "build-ts": "tsc -b && yarn clean-ts",
    "watch-ts": "tsc -b -w",
    "lint": "eslint --cache --max-warnings 0 --ext .js,.jsx,.ts,.tsx ./",
    "lint-fix": "yarn lint --fix"
  },
  "dependencies": {
    "@salto-io/adapter-api": "0.3.19",
    "@salto-io/adapter-components": "0.3.19",
    "@salto-io/adapter-utils": "0.3.19",
    "@salto-io/e2e-credentials-store": "0.3.19",
    "@salto-io/logging": "0.3.19",
    "@salto-io/lowerdash": "0.3.19",
    "joi": "^17.4.0",
    "lodash": "^4.17.21",
    "uuid": "^8.3.0",
    "wu": "^2.1.0",
    "xml-js": "^1.6.11"
  },
  "devDependencies": {
    "@salto-io/test-utils": "0.3.19",
    "@types/jest": "^27.4.0",
    "@types/lodash": "^4.14.168",
    "@typescript-eslint/eslint-plugin": "4.22.1",
    "@typescript-eslint/parser": "4.22.1",
    "axios": "^0.21.3",
    "axios-mock-adapter": "^1.19.0",
    "eslint": "^6.2.2",
    "eslint-config-airbnb": "18.0.1",
    "eslint-plugin-header": "^3.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^22.6.4",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^1.7.0",
    "jest": "^27.4.5",
    "jest-circus": "^27.4.5",
    "jest-extended": "^1.2.0",
    "jest-junit": "^12.0.0",
    "ts-jest": "^27.1.2",
    "tsc-watch": "^2.2.1",
    "typescript": "3.9.3"
  }
}
