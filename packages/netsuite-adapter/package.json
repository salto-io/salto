{
  "name": "@salto-io/netsuite-adapter",
  "version": "0.5.0",
  "license": "SEE LICENSE IN LICENSE",
  "description": "Salto Netsuite adapter",
  "repository": {
    "type": "git",
    "url": "https://github.com/salto-io/salto_monorepo"
  },
  "files": [
    "types",
    "dist/src",
    "dist/index.*",
    "dist/e2e_test",
    "dist/test/utils.*"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "../../build_utils/turbo_run.sh build-ts ; ../../build_utils/turbo_run.sh lint",
    "test": "jest",
    "e2e-test": "RUN_E2E_TESTS=1 jest",
    "clean": "rm -rf ./dist .eslintcache",
    "watch-test": "yarn tsc-watch",
    "build-ts": "tsc -b",
    "watch-ts": "tsc -b -w",
    "lint": "eslint --max-warnings 0 ./",
    "lint-fix": "yarn lint --fix",
    "format": "prettier --write . --ignore-path=../../.prettierignore --ignore-path=../../.gitignore --ignore-path=.gitignore",
    "check-format": "prettier --check . --ignore-path=../../.prettierignore --ignore-path=../../.gitignore --ignore-path=.gitignore"
  },
  "dependencies": {
    "@salto-io/adapter-api": "0.5.0",
    "@salto-io/adapter-components": "0.5.0",
    "@salto-io/adapter-utils": "0.5.0",
    "@salto-io/file": "0.5.0",
    "@salto-io/logging": "0.5.0",
    "@salto-io/lowerdash": "0.5.0",
    "@salto-io/suitecloud-cli-legacy": "npm:@salto-io/suitecloud-cli@1.9.0-salto-4",
    "@salto-io/suitecloud-cli-new": "npm:@salto-io/suitecloud-cli@2.0.0-salto-1",
    "ajv": "^7.1.1",
    "async-lock": "^1.2.4",
    "axios": "^1.7.2",
    "axios-retry": "^4.4.2",
    "bottleneck": "^2.19.5",
    "compare-versions": "4.1.3",
    "fast-xml-parser": "^4.4.0",
    "he": "^1.2.0",
    "joi": "^17.4.0",
    "lodash": "^4.17.21",
    "moment-timezone": "^0.5.39",
    "node-gzip": "^1.1.2",
    "oauth-1.0a": "^2.2.6",
    "readdirp": "^3.1.1",
    "shell-quote": "^1.7.3",
    "uuid": "^10.0.0",
    "wu": "^2.1.0",
    "xml-js": "^1.6.11"
  },
  "devDependencies": {
    "@salto-io/e2e-credentials-store": "0.5.0",
    "@salto-io/element-test-utils": "0.5.0",
    "@salto-io/test-utils": "0.5.0",
    "@types/async-lock": "^1.1.2",
    "@types/he": "^1.1.1",
    "@types/jest": "^29.5.12",
    "@types/lodash": "^4.14.168",
    "@types/node": "^18.9.0",
    "@types/node-gzip": "^1.1.3",
    "@types/shell-quote": "^1.7.1",
    "@types/uuid": "^10.0.0",
    "axios-mock-adapter": "^1.19.0",
    "eslint": "^9.16.0",
    "jest": "^29.7.0",
    "prettier": "3.2.5",
    "ts-jest": "^29.2.0",
    "tsc-watch": "^2.2.1",
    "turbo": "^2.0.6",
    "typescript": "4.8.4"
  }
}
