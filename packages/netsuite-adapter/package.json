{
  "name": "@salto-io/netsuite-adapter",
  "version": "0.1.12",
  "license": "Apache-2.0",
  "description": "Salto Netsuite adapter",
  "repository": {
    "type": "git",
    "url": "https://github.com/salto-io/salto_monorepo"
  },
  "files": [
    "dist/**/*"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "prepare": "yarn run build",
    "prepublishOnly": "yarn test",
    "build": "yarn run lint && yarn run build-ts",
    "test": "jest",
    "clean": "rm -rf ./dist",
    "clean-ts": "node ./node_modules/.bin/ts-cleaner -d dist -s . -v",
    "clean-ts-test": "yarn run clean-ts && yarn run test",
    "watch-test": "yarn run tsc-watch --onSuccess 'yarn run clean-ts-test'",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "lint": "eslint --cache --max-warnings 0 --ext .js,.jsx,.ts,.tsx ./",
    "lint-fix": "yarn run lint --fix"
  },
  "dependencies": {
    "@oracle/suitecloud-sdk": "https://github.com/salto-io/netsuite-suitecloud-sdk.git",
    "@salto-io/adapter-api": "0.1.12",
    "@salto-io/adapter-utils": "0.1.12",
    "@salto-io/logging": "0.1.12",
    "@salto-io/lowerdash": "0.1.12",
    "lodash": "^4.17.11",
    "wu": "^2.1.0",
    "xml-js": "^1.6.11"
  },
  "devDependencies": {
    "@types/jest": "^24.0.0",
    "@types/lodash": "^4.14.133",
    "@types/node": "^12.7.1",
    "@typescript-eslint/eslint-plugin": "2.21.0",
    "@typescript-eslint/parser": "2.21.0",
    "eslint": "^6.2.2",
    "eslint-config-airbnb": "18.0.1",
    "eslint-plugin-header": "^3.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^22.6.4",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^1.7.0",
    "jest": "^24.0.0",
    "jest-circus": "^24.9.0",
    "jest-junit": "^6.4.0",
    "ts-cleaner": "^1.0.3",
    "tsc-watch": "^2.2.1",
    "typescript": "^3.8.3",
    "vm2": "^3.9.1"
  }
}
