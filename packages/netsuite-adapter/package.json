{
  "name": "@salto-io/netsuite-adapter",
  "version": "0.3.54",
  "license": "Apache-2.0",
  "description": "Salto Netsuite adapter",
  "repository": {
    "type": "git",
    "url": "https://github.com/salto-io/salto_monorepo"
  },
  "files": [
    "types",
    "dist/src",
    "dist/index.*",
    "dist/e2e_test",
    "dist/test/utils.*"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "yarn concurrently \"yarn lint\" \"yarn build-ts\"",
    "test": "jest",
    "e2e-test": "RUN_E2E_TESTS=1 jest",
    "clean": "rm -rf ./dist .eslintcache",
    "clean-ts": "../../build_utils/clean-old-ts.sh",
    "clean-ts-test": "yarn clean-ts && yarn test",
    "watch-test": "yarn tsc-watch --onSuccess 'yarn clean-ts-test'",
    "build-ts": "tsc -b && yarn clean-ts",
    "watch-ts": "tsc -b -w",
    "lint": "eslint --cache --max-warnings 0 --ext .js,.jsx,.ts,.tsx ./ --ignore-path ../../.eslintignore",
    "lint-fix": "yarn lint --fix",
    "format": "prettier --write . --ignore-path=../../.prettierignore --ignore-path=../../.gitignore --ignore-path=.gitignore",
    "check-format": "prettier --check . --ignore-path=../../.prettierignore --ignore-path=../../.gitignore --ignore-path=.gitignore"
  },
  "dependencies": {
    "@salto-io/adapter-api": "0.3.54",
    "@salto-io/adapter-components": "0.3.54",
    "@salto-io/adapter-utils": "0.3.54",
    "@salto-io/file": "0.3.54",
    "@salto-io/logging": "0.3.54",
    "@salto-io/lowerdash": "0.3.54",
    "@salto-io/suitecloud-cli": "1.8.0-salto-2",
    "ajv": "^7.1.1",
    "async-lock": "^1.2.4",
    "axios": "^0.26.1",
    "axios-retry": "^4.0.0",
    "bottleneck": "^2.19.5",
    "compare-versions": "4.1.3",
    "crypto": "^1.0.1",
    "fast-xml-parser": "^3.15.0",
    "he": "^1.2.0",
    "lodash": "^4.17.21",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.39",
    "node-gzip": "^1.1.2",
    "oauth-1.0a": "^2.2.6",
    "readdirp": "^3.1.1",
    "shell-quote": "^1.7.3",
    "uuid": "^8.3.0",
    "wu": "^2.1.0",
    "xml-js": "^1.6.11"
  },
  "devDependencies": {
    "@salto-io/e2e-credentials-store": "0.3.54",
    "@salto-io/test-utils": "0.3.54",
    "@types/async-lock": "^1.1.2",
    "@types/he": "^1.1.1",
    "@types/jest": "^27.4.0",
    "@types/lodash": "^4.14.168",
    "@types/node": "^12.7.1",
    "@types/node-gzip": "^1.1.0",
    "@types/sax": "^1.2.1",
    "@types/shell-quote": "^1.7.1",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "4.22.1",
    "@typescript-eslint/parser": "4.22.1",
    "axios-mock-adapter": "^1.19.0",
    "eslint": "^7.3.2",
    "eslint-config-airbnb": "18.0.1",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-header": "^3.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^22.6.4",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^1.7.0",
    "jest": "^27.4.5",
    "jest-circus": "^27.4.5",
    "jest-each": "^26.6.2",
    "jest-junit": "^12.0.0",
    "prettier": "3.2.5",
    "ts-jest": "^27.1.2",
    "tsc-watch": "^2.2.1",
    "typescript": "4.1.3"
  }
}
