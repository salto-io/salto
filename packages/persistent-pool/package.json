{
  "name": "@salto-io/persistent-pool",
  "version": "0.2.4",
  "license": "Apache-2.0",
  "description": "Keeps a persistent pool of objects",
  "repository": {
    "type": "git",
    "url": "https://github.com/salto-io/salto_monorepo"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist",
    "src"
  ],
  "main": "dist/src",
  "types": "dist/src/index.d.ts",
  "scripts": {
    "build": "yarn concurrently \"yarn lint\" \"yarn build-ts\"",
    "build-ts": "tsc -b && yarn clean-ts",
    "test": "jest",
    "clean": "rm -rf ./dist .eslintcache",
    "clean-ts": "../../build_utils/clean-old-ts.sh",
    "watch-ts": "tsc -b -w",
    "lint": "eslint --cache --max-warnings 0 --ext .js,.jsx,.ts,.tsx ./",
    "lint-fix": "yarn lint --fix"
  },
  "dependencies": {
    "@salto-io/lowerdash": "0.2.4",
    "aws-sdk": "^2.573.0",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "@types/jest": "^26.0.20",
    "@types/lodash": "^4.14.133",
    "@types/uuid": "^3.4.6",
    "@typescript-eslint/eslint-plugin": "2.31.0",
    "@typescript-eslint/parser": "2.31.0",
    "eslint": "^6.2.2",
    "eslint-config-airbnb": "18.0.1",
    "eslint-plugin-header": "^3.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^22.6.4",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^1.7.0",
    "jest": "^26.6.3",
    "jest-circus": "^26.6.3",
    "jest-dynalite": "^3.3.1",
    "jest-environment-node": "^26.6.2",
    "jest-junit": "^12.0.0",
    "typescript": "3.9.3"
  }
}
