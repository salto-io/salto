{
  "name": "@salto-io/persistent-pool",
  "version": "0.4.3",
  "license": "SEE LICENSE IN LICENSE",
  "description": "Keeps a persistent pool of objects",
  "repository": {
    "type": "git",
    "url": "https://github.com/salto-io/salto_monorepo"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist/src"
  ],
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "scripts": {
    "build": "../../build_utils/turbo_run.sh build-ts ; ../../build_utils/turbo_run.sh lint",
    "build-ts": "tsc -b",
    "test": "jest",
    "clean": "rm -rf ./dist .eslintcache",
    "watch-ts": "tsc -b -w",
    "lint": "eslint --cache --max-warnings 0 ./",
    "lint-fix": "yarn lint --fix",
    "format": "prettier --write . --ignore-path=../../.prettierignore --ignore-path=../../.gitignore --ignore-path=.gitignore",
    "check-format": "prettier --check . --ignore-path=../../.prettierignore --ignore-path=../../.gitignore --ignore-path=.gitignore"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "3.198.0",
    "@aws-sdk/lib-dynamodb": "3.198.0",
    "@salto-io/logging": "0.4.3",
    "@salto-io/lowerdash": "0.4.3",
    "@salto-io/test-utils": "0.4.3",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@aws-sdk/types": "3.198.0",
    "@eslint/compat": "^1.1.0",
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.6.0",
    "@tony.ganchev/eslint-plugin-header": "^3.1.2",
    "@types/jest": "^29.5.12",
    "@types/lodash": "^4.14.168",
    "@types/node": "^18.9.0",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^7.16.0",
    "@typescript-eslint/parser": "^7.16.0",
    "eslint": "^9.6.0",
    "eslint-config-airbnb": "18.0.1",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^22.6.4",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^1.7.0",
    "globals": "^15.8.0",
    "jest": "^29.7.0",
    "jest-circus": "^29.7.0",
    "jest-dynalite": "^3.6.1",
    "jest-junit": "^16.0.0",
    "prettier": "3.2.5",
    "ts-jest": "^29.2.0",
    "turbo": "^2.0.6",
    "typescript": "4.8.4"
  }
}
