{
  "name": "@salto-io/persistent-pool",
  "version": "0.3.28",
  "license": "Apache-2.0",
  "description": "Keeps a persistent pool of objects",
  "repository": {
    "type": "git",
    "url": "https://github.com/salto-io/salto_monorepo"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist",
    "src"
  ],
  "main": "dist/src",
  "types": "dist/src/index.d.ts",
  "scripts": {
    "build": "yarn concurrently \"yarn lint\" \"yarn build-ts\"",
    "build-ts": "tsc -b && yarn clean-ts",
    "test": "jest",
    "clean": "rm -rf ./dist .eslintcache",
    "clean-ts": "../../build_utils/clean-old-ts.sh",
    "watch-ts": "tsc -b -w",
    "lint": "eslint --cache --max-warnings 0 --ext .js,.jsx,.ts,.tsx ./",
    "lint-fix": "yarn lint --fix"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "3.198.0",
    "@aws-sdk/lib-dynamodb": "3.198.0",
    "@salto-io/logging": "0.3.28",
    "@salto-io/lowerdash": "0.3.28",
    "@salto-io/test-utils": "0.3.28",
    "aws-sdk": "^2.863.0",
    "uuid": "^8.3.0"
  },
  "devDependencies": {
    "@aws-sdk/types": "3.198.0",
    "@types/jest": "^27.4.0",
    "@types/lodash": "^4.14.168",
    "@types/node": "^12.7.1",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "4.22.1",
    "@typescript-eslint/parser": "4.22.1",
    "eslint": "^6.2.2",
    "eslint-config-airbnb": "18.0.1",
    "eslint-plugin-header": "^3.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^22.6.4",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^1.7.0",
    "jest": "^27.4.5",
    "jest-circus": "^27.4.5",
    "jest-dynalite": "^3.3.1",
    "jest-environment-node": "^27.4.4",
    "jest-junit": "^12.0.0",
    "ts-jest": "^27.1.2",
    "typescript": "4.1.3"
  }
