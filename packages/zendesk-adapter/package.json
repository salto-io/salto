{
  "name": "@salto-io/zendesk-adapter",
  "version": "0.3.58",
  "license": "Apache-2.0",
  "description": "Salto Zendesk adapter",
  "repository": {
    "type": "git",
    "url": "https://github.com/salto-io/salto"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist/src",
    "dist/index.*"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "yarn concurrently \"yarn lint\" \"yarn build-ts\"",
    "test": "jest",
    "e2e-test": "RUN_E2E_TESTS=1 jest",
    "clean": "rm -rf ./dist .eslintcache",
    "clean-ts": "../../build_utils/clean-old-ts.sh",
    "clean-ts-test": "yarn clean-ts && yarn test",
    "watch-test": "yarn tsc-watch --onSuccess 'yarn clean-ts-test'",
    "build-ts": "tsc -b && yarn clean-ts",
    "watch-ts": "tsc -b -w",
    "lint": "eslint --cache --max-warnings 0 --ext .js,.jsx,.ts,.tsx ./ --ignore-path ../../.eslintignore",
    "lint-fix": "yarn lint --fix",
    "format": "prettier --write . --ignore-path=../../.prettierignore --ignore-path=../../.gitignore --ignore-path=.gitignore",
    "check-format": "prettier --check . --ignore-path=../../.prettierignore --ignore-path=../../.gitignore --ignore-path=.gitignore"
  },
  "dependencies": {
    "@handlebars/parser": "^2.1.0",
    "@salto-io/adapter-api": "0.3.58",
    "@salto-io/adapter-components": "0.3.58",
    "@salto-io/adapter-utils": "0.3.58",
    "@salto-io/dag": "0.3.58",
    "@salto-io/file": "0.3.58",
    "@salto-io/logging": "0.3.58",
    "@salto-io/lowerdash": "0.3.58",
    "@salto-io/parser": "0.3.58",
    "acorn": "^8.11.3",
    "acorn-walk": "^8.3.2",
    "domhandler": "^4.2.2",
    "form-data": "^4.0.0",
    "htmlparser2": "^7.2.0",
    "joi": "^17.4.0",
    "jszip": "^3.2.2",
    "lodash": "^4.17.21",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.39",
    "uuid": "^8.3.0",
    "wu": "^2.1.0"
  },
  "devDependencies": {
    "@salto-io/e2e-credentials-store": "0.3.58",
    "@salto-io/test-utils": "0.3.58",
    "@salto-io/workspace": "0.3.58",
    "@types/jest": "^27.4.0",
    "@types/jszip": "^3.1.6",
    "@types/lodash": "^4.14.168",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "4.22.1",
    "@typescript-eslint/parser": "4.22.1",
    "axios": "^0.26.1",
    "axios-mock-adapter": "^1.19.0",
    "axios-retry": "^4.0.0",
    "eslint": "^7.3.2",
    "eslint-config-airbnb": "18.0.1",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-header": "^3.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^22.6.4",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^1.7.0",
    "jest": "^27.4.5",
    "jest-circus": "^27.4.5",
    "jest-junit": "^12.0.0",
    "prettier": "3.2.5",
    "ts-jest": "^27.1.2",
    "tsc-watch": "^2.2.1",
    "typescript": "4.1.3"
  }
}
