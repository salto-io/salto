{
  "$schema": "https://turborepo.org/schema.json",
  "tasks": {
    "build-ts": {
      "outputs": [
        "dist/**"
      ],
      "dependsOn": [
        "^build-ts"
      ]
    },
    "clean": {
      "cache": false
    },
    "lint": {
      "outputs": []
    },
    "check-format": {
      "outputs": [
        ".check-format.cache"
      ]
    },
    "test": {
      "dependsOn": ["^test"],
      "outputs": ["coverage/**"]
    }

  }
}
